@base <https://ns.inria.fr/hmas/> .

@prefix any-org: <organizations/any-org#> .
@prefix logistics-spec: <organizations/specifications/logistics#> .
@prefix log: <organizations/instances/log#> .

@prefix agent-d: <workspaces/cup-production-site/agents/agent-d#> .

@prefix hmas: <https://purl.org/hmas/> .

log:DeliveryGroup a any-org:DeliveryGroup .

log:DelivererMembership a logistics-spec:DelivererMembership ;
    hmas:isMembershipOf agent-d:agent ;
    hmas:isMembershipFor any-org:Deliverer ;
    hmas:isMembershipIn log:DeliveryGroup .

log:organization a logistics-spec:GenericLogisticsCompanyOrganization ;
    hmas:hasProfile log: ;
    hmas:hasOrganizationalValue any-org:IntegrityRespect ;
    hmas:providesRole any-org:Deliverer  ;
    hmas:isStructuredBy log:DeliveryGroup  .
