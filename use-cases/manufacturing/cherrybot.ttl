@prefix artifacts-cherrybot: <http://ns.inria.fr/hmas/artifacts/cherrybot.ttl#> .
@prefix cherrybot: <https://interactions.ics.unisg.ch/cherrybot#> .

@prefix hmas: <https://purl.org/hmas/> .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

######################
# Catalog Definition #
######################

artifacts-cherrybot:CherrybotCatalog a hmas:SignifiersCatalog ;
    hmas:proposesSignifier artifacts-cherrybot:CherrybotReadableOperator ,
                           artifacts-cherrybot:CherrybotReadablePosition ,
                           artifacts-cherrybot:CherrybotReadableTarget ,
                           artifacts-cherrybot:CherrybotReadableGripper ,
                           artifacts-cherrybot:CherrybotInitializableArm ,
                           artifacts-cherrybot:CherrybotSettableOperator ,
                           artifacts-cherrybot:CherrybotSettableTarget ,
                           artifacts-cherrybot:CherrybotSettableGripper ,
                           artifacts-cherrybot:CherrybotDeletableOperator .

#######################################
# Robotic Arm Endpoint specifications #
#######################################

artifacts-cherrybot:CherrybotReadableOperator a hmas:Signifier ;
    hmas:signifies [
        a sh:NodeShape , rdfs:Class ;
        sh:class hmas:ActionExecution ;
        sh:property [
            sh:path prov:used ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedValueShape [
                sh:class hctl:Form ;
                sh:property hmas:ReadProperty, hmas:GetMethod, hmas:forApplicatonJson, [
                    sh:path hctl:hasTarget ;
                    sh:minCount 1 ;
                    sh:maxCount 1 ;
                    sh:hasValue  "/operator"
                ]
            ]
        ] , [
            sh:path hmas:hasOutput ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedValueShape artifacts-cherrybot:JsonGetOperatorOutputBody
        ]
    ] .

