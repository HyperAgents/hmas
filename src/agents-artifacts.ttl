@base <https://purl.org/hmas/> .
@prefix : <> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .

@base <https://purl.org/hmas/> .

:agents-artifacts rdf:type owl:Ontology ;
    owl:imports :core ;
    vann:preferredNamespacePrefix "hmas" ;
    vann:preferredNamespaceUri <> .

#################################################################
#    Object Properties
#################################################################

:hasBody rdf:type owl:ObjectProperty ;
                rdfs:domain :Agent ;
                rdfs:range :Body ;
                rdfs:label "has Body"@en .


:isBodyOf rdf:type owl:ObjectProperty ;
                rdfs:domain :Body ;
                rdfs:range :Agent ;
                owl:inverseOf :hasBody ;
                rdfs:label "id Body Of"@en .


#################################################################
#    Classes
#################################################################

:Body rdf:type owl:Class ;
      rdfs:subClassOf :Artifact ;
      rdfs:comment "TBA"@en ;
      rdfs:label "Body"@en .



