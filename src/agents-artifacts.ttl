@base <https://purl.org/hmas/> .
@prefix : <> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vann: <http://purl.org/vocab/vann/> .

@base <https://purl.org/hmas/> .

:agents-artifacts rdf:type owl:Ontology ;
    owl:imports :core ;
    vann:preferredNamespacePrefix "hmas" ;
    vann:preferredNamespaceUri <> .

#################################################################
#    Object Properties
#################################################################

:hasBody rdf:type owl:ObjectProperty ;
         rdfs:domain :Agent ;
         rdfs:range :Body ;
         owl:inverseOf :isBodyOf ;
         rdfs:isDefinedBy :agents-artifacts ;
         rdfs:label "has Body"@en .


:isBodyOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Body ;
          rdfs:range :Agent ;
          owl:inverseOf :hasBody ;
          rdfs:isDefinedBy :agents-artifacts ;
          rdfs:label "is Body Of"@en .

:hasInitialisationParameters rdf:type owl:ObjectProperty ; 
                             rdfs:domain :Artifact ;
                             rdfs:range rdf:List ;
                             rdfs:isDefinedBy :agents-artifacts ;
                             rdfs:label "has Initialisation Parameters"@en .


#################################################################
#    Datatype Properties
#################################################################

:hasName rdf:type owl:DatatypeProperty ; 
         rdfs:domain :Artifact ;
         rdfs:range xsd:sting ;
         rdfs:isDefinedBy :agents-artifacts ;
         rdfs:label "has Name"@en .

:hasClass rdf:type owl:DatatypeProperty ; 
         rdfs:domain :Artifact ;
         rdfs:range xsd:sting ;
         rdfs:label "has Class"@en .


#################################################################
#    Classes
#################################################################

:Body rdf:type owl:Class ;
      rdfs:subClassOf :Artifact ;
      rdfs:comment "an artifact representing its owner in the environment. It offers affordances to other agents to interact with and observe the owner. It also contains information about the owner"@en ;
      rdfs:isDefinedBy :agents-artifacts ;
      rdfs:label "Body"@en .



