@prefix hmas: <https://purl.org/hmas/> .
@prefix jade: <https://purl.org/hmas/> .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .


:jade rdf:type owl:Ontology ;
    owl:imports :core ;
    vann:preferredNamespacePrefix "hmas" ;
    vann:preferredNamespaceUri <> .

jade:MessagingCatalog a hmas:SignifiersCatalog ;
    rdfs:isDefinedBy :jade ;
    hmas:proposesSignifier jade:AgentContactable .

jade:PerformativeMessagingCatalog a hmas:SignifiersCatalog ;
    rdfs:isDefinedBy :jade ;
    hmas:proposesSignifier jade:ReceivableRequest ,
                           jade:ReceivableAccept ,
                           jade:ReceivableRefuse ,
                           jade:ReceivableFailure ,
                           jade:ReceivableInformDone ,
                           jade:ReceivableInformResult .

jade:AgentContactable a hmas:Signifier ;
    rdfs:isDefinedBy :jade ;
    hmas:signifies [
        a sh:NodeShape , rdfs:Class ;
        sh:class hmas:ActionExecution ;
        sh:property [
            sh:path prov:used ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedValueShape [
                sh:class hctl:Form ;
                sh:property hmas:FipaHTTPProtocol, hmas:InvokeAction, [
                    sh:path hctl:hasTarget ;
                    sh:minCount 1 ;
                    sh:maxCount 1 ;
                    sh:hasValue "/acc"
                ]
            ]
        ] , [
            sh:path hmas:hasInput ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedValueShape hmas:ACLMessageStructure
        ]
    ] .

jade:ReceivableRequest a hmas:Signifier ;
    rdfs:isDefinedBy :jade ;
    hmas:signifies [
        a sh:NodeShape , rdfs:Class ;
        sh:class hmas:ActionExecution , dul:Action ;
        sh:property [
            sh:path prov:used ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedValueShape hmas:FIPAMessageForm
        ] , [
            sh:path hmas:hasInput ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedValueShape [
                sh:class hmas:ACLMessage  ;
                sh:property hmas:AgentFIPASenderMessage, hmas:Request, hmas:InteractionOrder, hmas:FIPASL
            ]
        ]
    ] .

