##################################
# Universals
##################################

:WorkspaceProfileSpecification a sh:NodeShape ;
    rdfs:isDefinedBy :jacamo ;
    sh:class hmas:ResourceProfile ;
    sh:property [ 
        sh:path hmas:isProfileOf ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype hmas:Workspace   
    ] .

:ArtifactProfileSpecification a sh:NodeShape ;
    rdfs:isDefinedBy :jacamo ;
    sh:class hmas:ResourceProfile ;
    sh:property [ 
        sh:path hmas:isProfileOf ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype hmas:Artifact
    ] .