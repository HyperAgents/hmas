#############################################
# Individuals - Artifact Signifiers Catalog #
#############################################

:artifactCatalog rdf:type hmas:SignifiersCatalog ;
                 rdfs:isDefinedBy : ;
                 rdfs:comment "A catalog that proposes signifiers revealing how to interact with artifacts."@en ;
                 rdfs:label "artifact catalog"@en ;
                 hmas:proposesSignifier :perceivableArtifact ,
                                        :focusableArtifact ,
                                        :unfocusableArtifact ,
                                        :observableArtifact ,
                                        :unobservableArtifact ,
                                        :updatableArtifact ,
                                        :deletableArtifact .

:perceivableArtifact rdf:type hmas:Signifier ;
                     rdfs:isDefinedBy : ;
                     rdfs:comment "The artifact affords perceiving it."@en ;
                     rdfs:label "perceivable"@en ;
                     hmas:signifies [
                         a sh:NodeShape , rdfs:Class ;
                         sh:class hmas:ActionExecution, :PerceiveArtifact ;
                         sh:property [
                             sh:path prov:used ;
                             sh:qualifiedMinCount 1 ;
                             sh:qualifiedMaxCount 1 ;
                             sh:qualifiedValueShape :perceiveResourceFormSpecification
                         ] , [
                           sh:path hmas:hasOutput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :artifactProfileSpecification
                       ] 
                   ].

:focusableArtifact rdf:type hmas:Signifier ;
                   rdfs:isDefinedBy : ;
                   rdfs:comment "The artifact affords focusing on it."@en ;
                   rdfs:label "focusable"@en ;
                   hmas:signifies [
                       a sh:NodeShape , rdfs:Class ;
                       sh:class hmas:ActionExecution, :Focus ;
                       sh:property [
                           sh:path prov:used ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :hubFormSpecification
                       ] , [
                           sh:path hmas:hasInput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :subscriptionSpecification
                       ] , [
                           sh:path hmas:hasOutput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :artifactProfileSpecification
                       ] 
                   ].

:unfocusableArtifact rdf:type hmas:Signifier ;
                     rdfs:isDefinedBy : ;
                     rdfs:comment "The artifact affords stopping focusing on it."@en ;
                     rdfs:label "focusable"@en ;
                     hmas:signifies [
                         a sh:NodeShape , rdfs:Class ;
                         sh:class hmas:ActionExecution, :Unfocus ;
                         sh:property [
                             sh:path prov:used ;
                             sh:qualifiedMinCount 1 ;
                             sh:qualifiedMaxCount 1 ;
                             sh:qualifiedValueShape :hubFormSpecification
                         ] , [
                             sh:path hmas:hasInput ;
                             sh:qualifiedMinCount 1 ;
                             sh:qualifiedMaxCount 1 ;
                             sh:qualifiedValueShape :unsubscriptionSpecification
                         ] , [
                             sh:path hmas:hasOutput ;
                             sh:qualifiedMinCount 1 ;
                             sh:qualifiedMaxCount 1 ;
                             sh:qualifiedValueShape :artifactProfileSpecification
                         ] 
                     ].

:observableArtifact rdf:type hmas:Signifier ;
                    rdfs:isDefinedBy : ;
                    rdfs:comment "The artifact affords observing it."@en ;
                    rdfs:label "observable"@en ;
                    hmas:signifies [
                       a sh:NodeShape , rdfs:Class ;
                       sh:class hmas:ActionExecution, :ObserveArtifact ;
                       sh:property [
                           sh:path prov:used ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :hubFormSpecification
                       ] , [
                           sh:path hmas:hasInput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :subscriptionSpecification
                       ] , [
                           sh:path hmas:hasOutput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :artifactProfileSpecification
                       ] 
                   ].

:unbservableArtifact rdf:type hmas:Signifier ;
                     rdfs:isDefinedBy : ;
                     rdfs:comment "The resource affords unobserving an artifact."@en ;
                     rdfs:label "unobservable"@en ;
                     hmas:signifies [
                          a sh:NodeShape , rdfs:Class ;
                          sh:class hmas:ActionExecution, :UnobservableArtifact ;
                          sh:property [
                              sh:path prov:used ;
                              sh:qualifiedMinCount 1 ;
                              sh:qualifiedMaxCount 1 ;
                              sh:qualifiedValueShape :hubFormSpecification
                          ] , [
                              sh:path hmas:hasInput ;
                              sh:qualifiedMinCount 1 ;
                              sh:qualifiedMaxCount 1 ;
                              sh:qualifiedValueShape :unsubscriptionSpecification
                          ] , [
                              sh:path hmas:hasOutput ;
                              sh:qualifiedMinCount 1 ;
                              sh:qualifiedMaxCount 1 ;
                              sh:qualifiedValueShape :artifactProfileSpecification
                          ] 
                     ].

:updatableArtifact a hmas:Signifier ;
                   rdfs:isDefinedBy : ;
                   rdfs:comment "The resource affords updating an artifact."@en ;
                   rdfs:label "updatable"@en ;
                   hmas:signifies [
                       a sh:NodeShape , rdfs:Class ;
                       sh:class hmas:ActionExecution, :UpdateArtifact ;
                       sh:property [
                           sh:path prov:used ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :updateResourceFormSpecification
                       ] , [
                           sh:path hmas:hasInput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :artifactProfileSpecification
                       ] , [
                           sh:path hmas:hasOutput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :artifactProfileSpecification
                       ] 
                   ].

:deletableArtifact a hmas:Signifier ;
                    rdfs:isDefinedBy : ;
                    rdfs:comment "The resource affords deleting an artifact."@en ;
                    rdfs:label "deletable"@en ;
                    hmas:signifies [
                        a sh:NodeShape , rdfs:Class ;
                        sh:class hmas:ActionExecution, :DeleteArtifact ;
                        sh:property [
                            sh:path prov:used ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :deleteResourceFormSpecification
                        ] , [
                            sh:path hmas:hasOutput ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :artifactProfileSpecification
                        ] 
                    ].

