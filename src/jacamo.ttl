:context a jsonld:Context ;
    rdfs:isDefinedBy : ;
    jsonld:version 1.1 ;
    jsonld:definition [
        a jsonld:TermDefinition;
        jsonld:term "artifactName" ;
        jsonld:iri <https://purl.org/hmas/jacamo/hasName>
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "hasClass";
        jsonld:iri <https://purl.org/hmas/jacamo/hasClass> 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "initParams" ;
        jsonld:iri <https://purl.org/hmas/jacamo/hasInitialisationParameters> ;
        jsonld:container jsonld:listContainerType  
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "hub.topic" ;
        jsonld:iri <http://example.org/websub/topic> 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "hub.mode" ;
        jsonld:iri <http://example.org/websub/mode> 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "hub.callback" ;
        jsonld:iri <http://example.org/websub/callback> 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "performative" ;
        jsonld:iri <https://purl.org/hmas/jacamo/hasPerformative> ;
        jsonld:type jsonld:idContainerType 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "sender" ;
        jsonld:iri <https://purl.org/hmas/jacamo/hasSender> ;
        jsonld:type jsonld:idContainerType 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "receiver" ;
        jsonld:iri <https://purl.org/hmas/jacamo/hasReceiver> ;
        jsonld:type jsonld:idContainerType 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "content" ;
        jsonld:iri <https://purl.org/hmas/jacamo/hasContent> 
    ], [
        a jsonld:TermDefinition ;
        jsonld:term "language" ;
        jsonld:iri <https://purl.org/hmas/jacamo/hasLanguage> ;
        jsonld:type jsonld:idContainerType 
    ].

