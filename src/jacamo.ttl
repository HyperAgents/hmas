##################################
# Universals - SHACL Node Shapes #
##################################

:platformProfileSpecification a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class hmas:ResourceProfile ;
    sh:property [ 
        sh:path hmas:isProfileOf ;
        sh:name "platform";
        sh:description "The platform described by the profile";
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:class hmas:HypermediaMASPlatform    
    ] .

:workspaceProfileSpecification a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class hmas:ResourceProfile ;
    sh:property [ 
        sh:path hmas:isProfileOf ;
        sh:name "workspace";
        sh:description "The workspace described by the profile";
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:class hmas:Workspace   
    ] .

:artifactProfileSpecification a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class hmas:ResourceProfile ;
    sh:property [ 
        sh:path hmas:isProfileOf ;
        sh:name "artifact";
        sh:description "The artifact described by the profile";
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:class hmas:Artifact
    ] .

:bodyProfileSpecification a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class hmas:ResourceProfile ;
    sh:property [ 
        sh:path hmas:isProfileOf ;
        sh:name "body";
        sh:description "The body described by the profile";
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:class :Body
    ], [ 
        sh:path :isBodyOf ;
        sh:name "agent";
        sh:description "The agent whose body is described by the profile";
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:class hmas:Agent
    ] .

:artifactCreationSpecification  a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class hmas:Artifact ;
    sh:property [
        sh:path :hasName ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ], [ 
        sh:path :hasClass ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string 
    ], [ 
        sh:path :hasInitialisationParameters ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype rdf:List
    ].

:subscriptionSpecification a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class websub:Subscription ;
    sh:property :hubSuscribeMode, :hubCallback , :hubTopic.

:unsubscriptionSpecification a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class websub:Unsubscription ;
    sh:property :hubUnsuscribeMode, :hubCallback , :hubTopic.

:ACLMessageSpecification rdf:type sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:class hmas:ACLMessage ;
    sh:property [
        sh:path :hasPerformative ;
        sh:minCount 1 ;
        sh:maxCount 1
    ] , [
        sh:path :hasSender ;
        sh:minCount 1 ;
        sh:maxCount 1
    ] , [
        sh:path :hasReceiver ;
        sh:maxCount 1
    ] , [
        sh:path :hasContent ;
        sh:minCount 1 ;
        sh:maxCount 1
    ] , [
        sh:path :hasLanguage ;
        sh:maxCount 1
    ] .

:perceiveResourceFormSpecification a sh:NodeShape ;
    sh:class hctl:Form ;
    sh:property interaction-http:ReadAction, interaction-http:GetMethod, interaction-http:forTextTurtle, [
        sh:path hctl:hasTarget ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:hasValue ""
    ].

:hubFormSpecification a sh:NodeShape ;
    sh:class hctl:Form ;
    sh:property interaction-http:InvokeAction, interaction-http:PostMethod, interaction-http:forTextTurtle, [
        sh:path hctl:hasTarget ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:hasValue "/hub/"
    ].

:updateResourceFormSpecification a sh:NodeShape ;
     sh:class hctl:Form ;
     sh:property interaction-http:InvokeAction, interaction-http:PutMethod, interaction-http:forTextTurtle, interaction-http:XAgentWebIDHeader, [
         sh:path hctl:hasTarget ;
         sh:minCount 1 ;
         sh:maxCount 1 ;
         sh:hasValue ""
     ].
                           
:deleteResourceFormSpecification a sh:NodeShape ;
    sh:class hctl:Form ;
    sh:property interaction-http:InvokeAction, interaction-http:DeleteMethod, interaction-http:XAgentWebIDHeader, [
        sh:path hctl:hasTarget ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:hasValue ""
    ].

:inboxFormSpecification a sh:NodeShape ;
    sh:class hctl:Form ;
    sh:property  hmas:InvokeAction, hmas:PutMethod, hmas:forApplicatonJson, [
        sh:path hctl:hasTarget ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:hasValue "/inbox"
    ].

######################################
# Universals - SHACL Property Shapes #
######################################

:hubSubscribeMode a sh:PropertyShape ;
    rdfs:isDefinedBy : ;
    sh:path websub:mode ;
    sh:name "hub.mode";
    sh:description "The goal of the subscription request is to subscribe";
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:hasValue "subscribe" .

:hubUnsubscribeMode a sh:PropertyShape ;
    rdfs:isDefinedBy : ;
    sh:path websub:mode ;
    sh:name "hub.mode";
    sh:description "The goal of the subscription request is to unsubscribe";
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:hasValue "unsubscribe" .

:hubCallback a sh:PropertyShape ;
    rdfs:isDefinedBy : ;
    sh:path websub:callback ;
    sh:name "hub.callback";
    sh:description "The subscriber's callback URL where content distribution notifications should be delivered";
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype xsd:string .

:hubTopic a sh:PropertyShape ;
    rdfs:isDefinedBy : ;
    sh:path websub:topic ;
    sh:name "hub.topic";
    sh:description "The topic URL that the subscriber wishes to subscribe to";
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype xsd:string .

:agentSender a sh:PropertyShape ;
             rdfs:isDefinedBy : ;
             sh:path :hasSender ;
             sh:name "sender";
             sh:description "The agent that sends the message";
             sh:minCount 1 ;
             sh:maxCount 1 ;
             sh:datatype hmas:Agent .

:agentReceiver a sh:PropertyShape ;
               rdfs:isDefinedBy : ;
               sh:path :hasReceiver ;
               sh:name "receiver";
               sh:description "The agent that receives the message";
               sh:maxCount 1 ;
               sh:datatype hmas:Agent .

:AgentSpeakContentLanguage a sh:PropertyShape ;
                           rdfs:isDefinedBy : ;
                           sh:path :hasLanguage ;
                           sh:name "language";
                           sh:description "The content of the message is in the AgentSpeak language";
                           sh:maxCount 1 ;
                           sh:hasValue :AgentSpeak .

