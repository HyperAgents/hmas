                       sh:property [
                           sh:path prov:used ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :inboxFormSpecification 
                       ] , [
                           sh:path hmas:hasInput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape [
                              sh:class :ACLMessage  ;
