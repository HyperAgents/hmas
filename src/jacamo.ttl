:messagingCatalog a hmas:SignifiersCatalog ;
                  rdfs:isDefinedBy : ;
                  hmas:proposesSignifier :agentContactable .

:FIPARequestProtocolCatalog a hmas:SignifiersCatalog ;
                            rdfs:isDefinedBy : ;
                            hmas:proposesSignifier :receivableRequest ,
                                                   :receivableAccept ,
                                                   :receivableRefuse ,
                                                   :receivableFailure ,
                                                   :receivableInformDone .

:agentContactable a hmas:Signifier ;
                  rdfs:isDefinedBy : ;
                  hmas:signifies [
                      a sh:NodeShape ;
                      sh:class hmas:ActionExecution, :SendMessage ;
                      sh:property [
                          sh:path prov:used ;
                          sh:qualifiedMinCount 1 ;
                          sh:qualifiedMaxCount 1 ;
                          sh:qualifiedValueShape :inboxFormSpecification 
                      ] , [
                          sh:path hmas:hasInput ;
                          sh:qualifiedMinCount 1 ;
                          sh:qualifiedMaxCount 1 ;
                          sh:qualifiedValueShape :ACLMessageSpecification 
                      ]
                  ].

:receivableRequest a hmas:Signifier ;
