@base <https://purl.org/hmas/jacamo/> .
@prefix : <> .

@prefix hmas: <https://purl.org/hmas/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix vann: <http://purl.org/vocab/vann/> .

@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix jsonld: <http://www.w3.org/ns/json-ld#> .
@prefix interaction-http: <https://purl.org/hmas/interaction-http/> .

@prefix websub: <http://example.org/websub/> .
@prefix fipa: <https://purl.org/hmas/fipa/> .


: rdf:type owl:Ontology ;
  owl:imports hmas:core, hmas:interaction ;
  vann:preferredNamespacePrefix "jacamo" ;
  vann:preferredNamespaceUri <> .

###########
# Classes #
###########

:Body rdf:type owl:Class ;
      rdfs:isDefinedBy : ;
      rdfs:subClassOf hmas:Artifact ;
      rdfs:comment "An artifact representing the context of an agent in the environment. It enables the embodied agent to interact in the environment, and other agents to perceive and interact with the agent."@en ;
      rdfs:label "body"@en .

:CommunicativeAct rdf:type owl:Class ;
                  rdfs:isDefinedBy : ;
                  rdfs:comment "A KQML communicative act that can be used as a performative for communicating with Jason agents."@en ;
                  rdfs:label "communicative act"@en .

:Language rdf:type owl:Class ;
          rdfs:isDefinedBy : ;
          rdfs:comment "The language in which the content of an ACL message is expressed, for example, the AgentSpeak language"@en ;
          rdfs:label "language"@en .

:ACLMessage rdf:type owl:Class ;
            rdfs:isDefinedBy : ;
            rdfs:comment "A message that uses the KQML Agent Communication Language and that can be used for communication Jason agents."@en ;
            rdfs:label "ACL Message"@en .

:SendMessage rdf:type owl:Class ;
             rdfs:isDefinedBy : ;
             rdfs:subClassOf hmas:ActionExecution ;
             rdfs:comment "The execution of the action to send a message to an agent."@en ;
             rdfs:label "send message"@en .

:Focus rdf:type owl:Class ;
       rdfs:isDefinedBy : ;
       rdfs:subClassOf hmas:ActionExecution ;
       rdfs:comment "The execution of the action to focus on an artifact for observing the properties of the artifact."@en ;
       rdfs:label "focus"@en .

:Unfocus rdf:type owl:Class ;
         rdfs:isDefinedBy : ;
         rdfs:subClassOf hmas:ActionExecution ;
         rdfs:comment "The execution of the action to stop focusing on an artifact."@en ;
         rdfs:label "unfocus"@en .

:Perceive rdf:type owl:Class ;
          rdfs:isDefinedBy : ;
          rdfs:subClassOf hmas:ActionExecution ;
          rdfs:comment "The execution of the action to perceive a resource in the environment, for example, by perceiving the profile of the resource."@en ;
          rdfs:label "perceive"@en .

:Observe rdf:type owl:Class ;
         rdfs:isDefinedBy : ;
         rdfs:subClassOf hmas:ActionExecution ;
         rdfs:comment "The execution of the action to observe a resource in the environment, for example, by observing the profile of the resource."@en ;
         rdfs:label "observe"@en .

:Unobserve rdf:type owl:Class ;
         rdfs:isDefinedBy : ;
         rdfs:subClassOf hmas:ActionExecution ;
         rdfs:comment "The execution of the action to unobserve a resource in the environment, for example, by stopping observing the profile of the resource."@en ;
         rdfs:label "unobserve"@en .

:Update rdf:type owl:Class ;
        rdfs:isDefinedBy : ;
        rdfs:subClassOf hmas:ActionExecution ;
        rdfs:comment "The execution of the action to update a resource in the environment, for example, by updating the profile of the resource."@en ;
        rdfs:label "update"@en .

:Delete rdf:type owl:Class ;
        rdfs:isDefinedBy : ;
        rdfs:subClassOf hmas:ActionExecution ;
        rdfs:comment "The execution of the action to delete a resource in the environment, for example, by deleting the profile of the resource."@en ;
        rdfs:label "delete"@en .

:PerceivePlatform rdf:type owl:Class ;
                  rdfs:isDefinedBy : ;
                  rdfs:subClassOf :Perceive ;
                  rdfs:comment "The execution of the action to perceive a platform, for example, by perceiving the profile of the platform."@en ;
                  rdfs:label "update workspace"@en .

:PerceiveWorkspace rdf:type owl:Class ;
                   rdfs:isDefinedBy : ;
                   rdfs:subClassOf :Perceive ;
                   rdfs:comment "The execution of the action to perceive a workspace, for example, by perceiving the profile of the workspace."@en ;
                   rdfs:label "perceive workspace"@en .

:PerceiveArtifact rdf:type owl:Class ;
                  rdfs:isDefinedBy : ;
                  rdfs:subClassOf :Perceive ;
                  rdfs:comment "The execution of the action to perceive an artifact, for example, by perceiving the profile of the artifact."@en ;
                  rdfs:label "perceive artifact"@en .

:ObservePlatform rdf:type owl:Class ;
                 rdfs:isDefinedBy : ;
                 rdfs:subClassOf :Observe ;
                 rdfs:comment "The execution of the action to observe a platform, for example, by observing the profile of the platform to receive notifications about the creation or deletion of workspaces hosted by the platform."@en ;
                 rdfs:label "observe platform"@en .

:ObserveWorkspace rdf:type owl:Class ;
                 rdfs:isDefinedBy : ;
                 rdfs:subClassOf :Observe ;
                 rdfs:comment "The execution of the action to observe a workspace, for example, by observing the profile of the workspace to receive notifaction about the creation, registration or deletion of artifacts and sub-workspaces in the workspace."@en ;
                 rdfs:label "observe workspace"@en .

:ObserveArtifact rdf:type owl:Class ;
                 rdfs:isDefinedBy : ;
                 rdfs:subClassOf :Observe ;
                 rdfs:comment "The execution of the action to observe an artifact, for example, by observing the profile of the artifact to receive notifications about the exposure or hiding of signifiers in the profile."@en ;
                 rdfs:label "observe artifact"@en .

:UnobservePlatform rdf:type owl:Class ;
                   rdfs:isDefinedBy : ;
                   rdfs:subClassOf :Unobserve ;
                   rdfs:comment "The execution of the action to unobserve a platform, for example, by stopping observing the profile of the platform."@en ;
                   rdfs:label "unobserve platform"@en .

:UnobserveWorkspace rdf:type owl:Class ;
                    rdfs:isDefinedBy : ;
                    rdfs:subClassOf :Unobserve ;
                    rdfs:comment "The execution of the action to unobserve a workspace, for example, by stopping observing the profile of the workspace."@en ;
                    rdfs:label "unobserve workspace"@en .

:UnobserveArtifact rdf:type owl:Class ;
                   rdfs:isDefinedBy : ;
                   rdfs:subClassOf :Unobserve ;
                   rdfs:comment "The execution of the action to unobserve an artifact, for example, by stopping observing the profile of the artifact."@en ;
                   rdfs:label "unobserve artifact"@en .

:UpdateWorkspace rdf:type owl:Class ;
                 rdfs:isDefinedBy : ;
                 rdfs:subClassOf :Update ;
                 rdfs:comment "The execution of the action to update a workspace, for example, by updating the profile of the workspace."@en ;
                 rdfs:label "update workspace"@en .

:UpdateArtifact rdf:type owl:Class ;
                rdfs:isDefinedBy : ;
                rdfs:subClassOf :Update ;
                rdfs:comment "The execution of the action to update a artifact, for example, by updating the profile of the artifact."@en ;
                rdfs:label "update artifact"@en .

:DeleteWorkspace rdf:type owl:Class ;
                 rdfs:isDefinedBy : ;
                 rdfs:subClassOf :Delete ;
                 rdfs:comment "The execution of the action to delete a workspace, for example, by deleting the profile of the workspace."@en ;
                 rdfs:label "delete workspace"@en .

:DeleteArtifact rdf:type owl:Class ;
                rdfs:isDefinedBy : ;
                rdfs:subClassOf :Delete ;
                rdfs:comment "The execution of the action to delete an artifact, for example, by deleting the profile of the artifact."@en ;
                rdfs:label "delete artifact"@en .

:RegisterArtifact rdf:type owl:Class ;
                  rdfs:isDefinedBy : ;
                  rdfs:subClassOf hmas:ActionExecution ;
                  rdfs:comment "The execution of the action to register an existing artifact in a workspace."@en ;
                  rdfs:label "register artifact"@en .

:MakeArtifact rdf:type owl:Class ;
              rdfs:isDefinedBy : ;
              rdfs:subClassOf hmas:ActionExecution ;
              rdfs:comment "The execution of the action to make new artifacts in a workspace."@en ;
              rdfs:label "make artifact"@en .

:MakeWorkspace rdf:type owl:Class ;
               rdfs:isDefinedBy : ;
               rdfs:subClassOf hmas:ActionExecution ;
               rdfs:comment "The execution of the action to make new workspaces."@en ;
               rdfs:label "make workspace"@en .

:JoinWorkspace rdf:type owl:Class ;
                rdfs:isDefinedBy : ;
                rdfs:subClassOf hmas:ActionExecution ;
                rdfs:comment "The execution of the action to join a workspace."@en ;
                rdfs:label "join workspace"@en .

:LeaveWorkspace rdf:type owl:Class ;
                rdfs:isDefinedBy : ;
                rdfs:subClassOf hmas:ActionExecution ;
                rdfs:comment "The execution of the action to leave a workspace."@en ;
                rdfs:label "leave workspace"@en .

#####################
# Object Properties #
#####################

:hasBody rdf:type owl:ObjectProperty ;
         rdfs:isDefinedBy : ;
         rdfs:domain hmas:Agent ;
         rdfs:range :Body ;
         owl:inverseOf :isBodyOf ;
         rdfs:comment "A relation between an agent and its body."@en ;
         rdfs:label "has body"@en .

:isBodyOf rdf:type owl:ObjectProperty ;
          rdfs:isDefinedBy : ;
          rdfs:domain :Body ;
          rdfs:range hmas:Agent ;
          owl:inverseOf :hasBody ;
          rdfs:comment "A relation between a body and the agent who has the body."@en ;
          rdfs:label "is body of"@en .

:hasInitialisationParameters rdf:type owl:ObjectProperty ; 
                             rdfs:isDefinedBy : ;
                             rdfs:domain hmas:Artifact ;
                             rdfs:range rdf:List ;
                             rdfs:comment "A relation between an artifact and its initialization parameters."@en ;
                             rdfs:label "has initialisation parameters"@en .

#######################
# Datatype Properties #
#######################

:hasName rdf:type owl:DatatypeProperty ; 
         rdfs:isDefinedBy : ;
         rdfs:domain hmas:Artifact ;
         rdfs:range xsd:sting ;
         rdfs:comment "A relation between an artifact and its name."@en ;
         rdfs:label "has name"@en .

:hasClass rdf:type owl:DatatypeProperty ; 
         rdfs:isDefinedBy : ;
         rdfs:domain hmas:Artifact ;
         rdfs:range xsd:sting ;
         rdfs:comment "A relation between an artifact and its class."@en ;
         rdfs:label "has class"@en .

###############
# Individuals #
###############

:AgentSpeak rdf:type :Language ;
        rdfs:isDefinedBy : ;
        rdfs:comment "The AgentSpeak language"@en ;
        rdfs:label "AgentSpeak"@en .

##########################################
# Individuals - Jason-KQML Performatives #
##########################################

:tell rdf:type :CommunicativeAct ;
      rdfs:isDefinedBy : ;
      rdfs:comment "A communicative act with which the sender intends the receiver to believe (that the sender believes) the literal in the message’s content to be true."@en ;
      rdfs:label "tell"@en .

:untell rdf:type :CommunicativeAct ;
        rdfs:isDefinedBy : ;
        rdfs:comment "A communicative act with which the sender intends the receiver not to believe (that the sender believes) the literal in the message’s content to be true."@en ;
        rdfs:label "untell"@en .

:achieve rdf:type :CommunicativeAct ;
         rdfs:isDefinedBy : ;
         rdfs:comment "A communicative act with which the sender requests the receiver to try and achieve a state of affairs where the literal in the message content is true (i.e. the sender is delegating a goal to the receiver)."@en ;
         rdfs:label "acheive"@en .

:unachieve rdf:type :CommunicativeAct ;
           rdfs:isDefinedBy : ;
           rdfs:comment "A communicative act with which the sender requests the receiver to drop the goal of achieving a state of affairs where the message content is true."@en ;
           rdfs:label "unachieve"@en .

:askOne rdf:type :CommunicativeAct ;
        rdfs:isDefinedBy : ;
        rdfs:comment "A communicative act with which the sender wants to know if the content of the message is true for the receiver (i.e. if there is an answer that makes the content a logical consequence of the receiver’s belief base, by appropriate substitution of variables)."@en ;
        rdfs:label "askOne"@en .

:askAll rdf:type :CommunicativeAct ;
        rdfs:isDefinedBy : ;
        rdfs:comment "A communicative act with which the sender wants all of the receiver’s answers to a question."@en ;
        rdfs:label "askAll"@en .

:tellHow rdf:type :CommunicativeAct ;
         rdfs:isDefinedBy : ;
         rdfs:comment "A communicative act with which the sender informs the receiver of a plan (the sender’s know-how)."@en ;
         rdfs:label "tellHow"@en .

:untellHow rdf:type :CommunicativeAct ;
           rdfs:isDefinedBy : ;
           rdfs:comment "A communicative act with which the sender requests the receiver to disregard a certain plan (i.e. delete that plan from its plan library)."@en ;
           rdfs:label "untellHow"@en .

:askHow rdf:type :CommunicativeAct ;
        rdfs:isDefinedBy : ;
        rdfs:comment "A communicative act with which the sender wants all of the receiver’s plans that are relevant for the triggering event in the message content."@en ;
        rdfs:label "askHow"@en .

#############################################
# Individuals - Platform Signifiers Catalog #
#############################################

:platformCatalog rdf:type hmas:SignifiersCatalog ;
                 rdfs:isDefinedBy : ;
                 rdfs:comment "A catalog that proposes signifiers revealing how to interact with platforms."@en ;
                 rdfs:label "platform catalog"@en ;
                 hmas:proposesSignifier :perceivablePlatform ,
                                        :observablePlatform ,
                                        :unobservablePlatform ,
                                        :workspaceHostablePlatform .

:perceivablePlatform rdf:type hmas:Signifier ;
                     rdfs:isDefinedBy : ;
                     rdfs:comment "The platform affords perceiving it."@en ;
                     rdfs:label "perceivable"@en ;
                     hmas:signifies [
                         a sh:NodeShape , rdfs:Class ;
                         sh:class hmas:ActionExecution, :PerceivePlatform ;
                         sh:property [
                             sh:path prov:used ;
                             sh:qualifiedMinCount 1 ;
                             sh:qualifiedMaxCount 1 ;
                             sh:qualifiedValueShape :perceiveResourceFormSpecification
                         ] , [
                           sh:path hmas:hasOutput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :platformProfileSpecification
                       ] 
                   ].

:observablePlatform rdf:type hmas:Signifier ;
                    rdfs:isDefinedBy : ;
                    rdfs:comment "The platform affords observing it."@en ;
                    rdfs:label "observable"@en ;
                    hmas:signifies [
                       a sh:NodeShape , rdfs:Class ;
                       sh:class hmas:ActionExecution, :ObservePlatform ;
                       sh:property [
                           sh:path prov:used ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :hubFormSpecification
                       ] , [
                           sh:path hmas:hasInput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :subscriptionSpecification
                       ] , [
                           sh:path hmas:hasOutput ;
                           sh:qualifiedMinCount 1 ;
                           sh:qualifiedMaxCount 1 ;
                           sh:qualifiedValueShape :workspaceProfileSpecification
                       ] 
                   ].

:unbservablePlatform rdf:type hmas:Signifier ;
                     rdfs:isDefinedBy : ;
                     rdfs:comment "The platform affords unobserving it."@en ;
                     rdfs:label "unobservable"@en ;
                     hmas:signifies [
                          a sh:NodeShape , rdfs:Class ;
                          sh:class hmas:ActionExecution, :UnobservablePlatform ;
                          sh:property [
                              sh:path prov:used ;
                              sh:qualifiedMinCount 1 ;
                              sh:qualifiedMaxCount 1 ;
                              sh:qualifiedValueShape :hubFormSpecification
                          ] , [
                              sh:path hmas:hasInput ;
                              sh:qualifiedMinCount 1 ;
                              sh:qualifiedMaxCount 1 ;
                              sh:qualifiedValueShape :unsubscriptionSpecification
                          ] , [
                              sh:path hmas:hasOutput ;
                              sh:qualifiedMinCount 1 ;
                              sh:qualifiedMaxCount 1 ;
                              sh:qualifiedValueShape :workspaceProfileSpecification
                          ] 
                     ].

:workspaceHostablePlatform rdf:type hmas:Signifier ;
                           rdfs:isDefinedBy : ;
                           rdfs:comment "The platform affords making new workspaces."@en ;
                           rdfs:label "hosting-able"@en ;
                           hmas:signifies [
                               a sh:NodeShape , rdfs:Class ;
                               sh:class hmas:ActionExecution, :MakeWorkspace ;
                               sh:property [
                                   sh:path prov:used ;
                                   sh:qualifiedMinCount 1 ;
                                   sh:qualifiedMaxCount 1 ;
                                   sh:qualifiedValueShape [
                                       sh:class hctl:Form ;
                                       sh:property interaction-http:InvokeAction, interaction-http:PostMethod, interaction-http:forTextTurtle, interaction-http:SlugHeader, interaction-http:XAgentWebIDHeader, [
                                           sh:path hctl:hasTarget ;
                                           sh:minCount 1 ;
                                           sh:maxCount 1 ;
                                           sh:hasValue ""
                                           ]
                                       ]
                                   ] , [
                                       sh:path hmas:hasOutput ;
                                       sh:qualifiedMinCount 1 ;
                                       sh:qualifiedMaxCount 1 ;
                                       sh:qualifiedValueShape :workspaceProfileSpecification
                                   ] 
                               ].

##############################################
# Individuals - Workspace Signifiers Catalog #
##############################################

:workspaceCatalog rdf:type hmas:SignifiersCatalog ;
                  rdfs:isDefinedBy : ;
                  rdfs:comment "A catalog that proposes signifiers revealing how to interact with workspaces."@en ;
                  rdfs:label "workspace catalog"@en ;
                  hmas:proposesSignifier :perceivableWorkspace ,
                                         :observableWorkspace ,
                                         :unobservableWorkspace ,
                                         :updatableWorkspace ,
                                         :deletableWorkspace ,
                                         :artifactRegisterableWorkspace ,
                                         :artifactCreatableWorkspace ,
                                         :partitionableWorkspace , 
                                         :joinableWorkspace ,
                                         :leavableWorkspace .

:perceivableWorkspace rdf:type hmas:Signifier ;
                      rdfs:isDefinedBy : ;
                      rdfs:comment "The workspace affords perceiving it."@en ;
                      rdfs:label "perceivable"@en ;
                      hmas:signifies [
                          a sh:NodeShape , rdfs:Class ;
                          sh:class hmas:ActionExecution, :PerceiveWorkspace ;
                          sh:property [
                              sh:path prov:used ;
                              sh:qualifiedMinCount 1 ;
                              sh:qualifiedMaxCount 1 ;
                              sh:qualifiedValueShape :perceiveResourceFormSpecification
                          ] , [
                            sh:path hmas:hasOutput ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :workspaceProfileSpecification
                        ] 
                    ].

:observableWorkspace rdf:type hmas:Signifier ;
                     rdfs:isDefinedBy : ;
                     rdfs:comment "The workspace affords observing it."@en ;
                     rdfs:label "observable"@en ;
                     hmas:signifies [
                        a sh:NodeShape , rdfs:Class ;
                        sh:class hmas:ActionExecution, :ObserveWorkspace ;
                        sh:property [
                            sh:path prov:used ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :hubFormSpecification
                        ] , [
                            sh:path hmas:hasInput ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :subscriptionSpecification
                        ] , [
                            sh:path hmas:hasOutput ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :workspaceProfileSpecification
                        ] 
                    ].

    :unbservableWorkspace rdf:type hmas:Signifier ;
                          rdfs:isDefinedBy : ;
                          rdfs:comment "The workspace affords unobserving it."@en ;
                          rdfs:label "unobservable"@en ;
                          hmas:signifies [
                              a sh:NodeShape , rdfs:Class ;
                              sh:class hmas:ActionExecution, :UnobservableWorkspace ;
                              sh:property [
                                  sh:path prov:used ;
                                  sh:qualifiedMinCount 1 ;
                                  sh:qualifiedMaxCount 1 ;
                                  sh:qualifiedValueShape :hubFormSpecification
                              ] , [
                                  sh:path hmas:hasInput ;
                                  sh:qualifiedMinCount 1 ;
                                  sh:qualifiedMaxCount 1 ;
                                  sh:qualifiedValueShape :unsubscriptionSpecification
                              ] , [
                                  sh:path hmas:hasOutput ;
                                  sh:qualifiedMinCount 1 ;
                                  sh:qualifiedMaxCount 1 ;
                                  sh:qualifiedValueShape :workspaceProfileSpecification
                              ] 
                         ].

:updatableWorkspace a hmas:Signifier ;
                    rdfs:isDefinedBy : ;
                    rdfs:comment "The workspace affords updating it."@en ;
                    rdfs:label "updatable"@en ;
                    hmas:signifies [
                        a sh:NodeShape , rdfs:Class ;
                        sh:class hmas:ActionExecution, :UpdateWorkspace ;
                        sh:property [
                            sh:path prov:used ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :updateResourceFormSpecification
                        ] , [
                            sh:path hmas:hasInput ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :workspaceProfileSpecification
                        ] , [
                            sh:path hmas:hasOutput ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :workspaceProfileSpecification
                        ] 
                    ].

:deletableWorkspace a hmas:Signifier ;
                    rdfs:isDefinedBy : ;
                    rdfs:comment "The workspace affords deleting it."@en ;
                    rdfs:label "deletable"@en ;
                    hmas:signifies [
                        a sh:NodeShape , rdfs:Class ;
                        sh:class hmas:ActionExecution, :DeleteWorkspace ;
                        sh:property [
                            sh:path prov:used ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :deleteResourceFormSpecification
                        ] , [
                            sh:path hmas:hasOutput ;
                            sh:qualifiedMinCount 1 ;
                            sh:qualifiedMaxCount 1 ;
                            sh:qualifiedValueShape :workspaceProfileSpecification
                        ] 
                    ].

:artifactRegisterableWorkspace a hmas:Signifier ;
                               rdfs:isDefinedBy : ;
                               rdfs:comment "The workspace affords resigering an artifact in it."@en ;
                               rdfs:label "(registered) artifact containable"@en ;
                               hmas:signifies [
                                   a sh:NodeShape , rdfs:Class ;
                                   sh:class hmas:ActionExecution, :RegisterArtifact ;
                                   sh:property [
                                       sh:path prov:used ;
                                       sh:qualifiedMinCount 1 ;
                                       sh:qualifiedMaxCount 1 ;
                                       sh:qualifiedValueShape [
                                           sh:class hctl:Form ;
                                           sh:property interaction-http:InvokeAction, interaction-http:PostMethod, interaction-http:forTextTurtle, interaction-http:SlugHeader, interaction-http:XAgentWebIDHeader, [
                                               sh:path hctl:hasTarget ;
                                               sh:minCount 1 ;
                                               sh:maxCount 1 ;
                                               sh:hasValue "/artifacts/"
                                          ]
                                       ]
                                   ] , [
                                       sh:path hmas:hasInput ;
                                       sh:qualifiedMinCount 1 ;
                                       sh:qualifiedMaxCount 1 ;
                                       sh:qualifiedValueShape :artifactProfileSpecification
                                    ], [
                                       sh:path hmas:hasOutput ;
                                       sh:qualifiedMinCount 1 ;
                                       sh:qualifiedMaxCount 1 ;
                                       sh:qualifiedValueShape :artifactProfileSpecification
                                    ] 
                                ].

:artifactCreatableWorkspace a hmas:Signifier ;
                            rdfs:isDefinedBy : ;
                            rdfs:comment "The workspace affords making artifacts in it."@en ;
                            rdfs:label "(new) artifact containable"@en ;
                            hmas:signifies [
                                a sh:NodeShape , rdfs:Class ;
                                sh:class hmas:ActionExecution, :MakeArtifact ;
                                sh:property [
                                    sh:path prov:used ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedMaxCount 1 ;
                                    sh:qualifiedValueShape [
                                        sh:class hctl:Form ;
                                        sh:property interaction-http:InvokeAction, interaction-http:PostMethod, interaction-http:forApplicatonJson, interaction-http:XAgentWebIDHeader, [
                                            sh:path hctl:hasTarget ;
                                            sh:minCount 1 ;
                                            sh:maxCount 1 ;
                                            sh:hasValue "/artifacts/"
                                        ]
                                    ]
                                ] , [
                                    sh:path hmas:hasInput ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedMaxCount 1 ;
                                    sh:qualifiedValueShape :artifactCreationSpecification
                                ] , [
                                    sh:path hmas:hasOutput ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedMaxCount 1 ;
                                    sh:qualifiedValueShape :artifactProfileSpecification
                                ] 
                            ].

:partitionableWorkspace a hmas:Signifier ;
                        rdfs:isDefinedBy : ;
                        rdfs:comment "The workspace affords making sub-workspaces in it."@en ;
                        rdfs:label "partitionable"@en ;
                        hmas:signifies [
                            a sh:NodeShape , rdfs:Class ;
                            sh:class hmas:ActionExecution, :MakeWorkspace ;
                            sh:property [
                                sh:path prov:used ;
                                sh:qualifiedMinCount 1 ;
                                sh:qualifiedMaxCount 1 ;
                                sh:qualifiedValueShape [
                                    sh:class hctl:Form ;
                                    sh:property interaction-http:InvokeAction, interaction-http:PostMethod, interaction-http:forTextTurtle, interaction-http:SlugHeader, interaction-http:XAgentWebIDHeader, [
                                        sh:path hctl:hasTarget ;
                                        sh:minCount 1 ;
                                        sh:maxCount 1 ;
                                        sh:hasValue ""
                                        ]
                                    ]
                                ] , [
                                    sh:path hmas:hasOutput ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedMaxCount 1 ;
                                    sh:qualifiedValueShape :workspaceProfileSpecification
                                ] 
                            ].

:joinableWorkspace a hmas:Signifier ;
