prefix : <https://purl.org/hmas/>
prefix ex: <http://example.org/>
prefix sh: <http://www.w3.org/ns/shacl#> 

select ?inputType
where {
    ex:moveGripperShape sh:property ?propertySpec .
    ?propertySpec sh:path :hasInput .
    ?propertySpec sh:qualifiedValueShape ?inputSpec .   
    ?inputSpec sh:class ?inputType .
}
order by ?inputType
