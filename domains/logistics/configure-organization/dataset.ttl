        ] .
ex:Delivering_Usage a sh:NodeShape, rdfs:Class ;
    rdfs:subClassOf hmas:Usage ;
    sh:property [
        sh:path hmas:isUsageBy ;
        sh:qualifiedValueShape [ sh:class hmas:Agent ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:isUsageOf ;
        sh:qualifiedValueShape [
            sh:and (
                [ sh:class hmas:Artifact ]
                [ sh:path hmas:hasFacility ;
                    sh:and (
                        [ sh:node ex:LiftUp ]
                        [ sh:node ex:LiftDown ]
                        [ sh:node ex:Move ]
                        )
                    ]
                )
            ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:isUsageOf ;
        sh:qualifiedValueShape [
            sh:and (
                [ sh:class hmas:Artifact ]
                [ sh:path hmas:hasFacility ;
                    sh:node ex:PrintLabel ;
                    ]
                ) ;
            ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:isUsageIn ;
        sh:qualifiedValueShape [ sh:class ex:PickingSetting ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] .

