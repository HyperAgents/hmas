@prefix sh: <http://www.w3.org/ns/shacl#> .

## SHACL SPARQL-Constraint prefixes
ex:
    sh:declare [
        sh:prefix "ex" ;
        sh:namespace "http://example.org/" ;
        ] .
hmas:
    sh:declare [
        sh:prefix "hmas" ;
        sh:namespace "https://purl.org/hmas/" ;
        ] .
rdfs:
    sh:declare [
        sh:prefix "rdfs" ;
        sh:namespace "http://www.w3.org/2000/01/rdf-schema#" ;
        ] .

