    hmas:isMemberOf ex:FL_Logistics ;
    hmas:isCommittedToMission ex:FL_AccountManager_HR_Hiring .
ex:Gabriel a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:FL_Logistics .
ex:Igor a hmas:Agent ;
