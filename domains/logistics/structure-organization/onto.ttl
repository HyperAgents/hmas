@base <https://purl.org/hmas/> .
@prefix : <> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

#################################################################
#    Classes
#################################################################

:Group a owl:Class ;
    rdfs:isDefinedBy :regulation ;
    rdfs:comment "A Group is a set of Roles that exists in an Organization."@en ;
    rdfs:label "group"@en, "groupe"@fr .
    
#################################################################
#    Properties
#################################################################

:structures a owl:ObjectProperty ;
    rdfs:isDefinedBy :regulation ;
    rdfs:domain :Group ;
    rdfs:range :Organization ;
    rdfs:inverseOf :isStructuredBy ;
    rdfs:comment "A relation that refers to a Group that structures an Organization."@en ;
    rdfs:label "structures"@en, "structure"@fr .

:isStructuredBy a owl:ObjectProperty ;
    rdfs:isDefinedBy :regulation ;
    rdfs:domain :Organization ;
    rdfs:range :Group ;
    rdfs:inverseOf :structures ;
    rdfs:comment "A relation that refers to an Organization that is structured by a Group."@en ;
    rdfs:label "is structured by"@en, "est structuré par"@fr .
    
:plays a owl:ObjectProperty ;
    rdfs:isDefinedBy :regulation ;
    rdfs:domain :Agent ;
    rdfs:range :Role ;
    rdfs:comment "A relation that refers to a Role that an Agent plays in a Group."@en ;
    rdfs:label "plays"@en, "joue"@fr .

:belongsTo a owl:ObjectProperty ;
    rdfs:isDefinedBy :regulation ;
    rdfs:domain :Role ;
    rdfs:range :Group ;
    rdfs:comment "A relation that refers to a Role that is part of a Group."@en ;
    rdfs:label "belongs to"@en, "appartient à"@fr .

:isLinkedTo a owl:ObjectProperty ;
    rdfs:isDefinedBy :regulation ;
    rdfs:domain :Role ;
    rdfs:range :Role ;
    rdfs:comment "A relation that refers to a relation between two Roles."@en ;
    rdfs:label "is linked to"@en, "est lié à"@fr .
    
:isSubGroupOf a owl:ObjectProperty ;
    rdfs:isDefinedBy :regulation ;
    rdfs:domain :Group ;
    rdfs:range :Group ;
    rdfs:comment "A relation that refers to a subgroup relationship between two Groups."@en ;
    rdfs:label "is subgroup of"@en, "est un sous-groupe de"@fr .
