prefix ex: <http://example.org/>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix hmas: <https://purl.org/hmas/>

select ?org
where {
  ex:SL_Logistics a ?slOrgModel ;
                  hmas:hasResourceProfile ?slOrgProfile .
  ?slOrgModel a sh:NodeShape ;
              rdfs:subClassOf hmas:Organization .
  
  ?org a ?orgModel ;
       hmas:hasResourceProfile ?orgProfile .
  ?orgModel a sh:NodeShape ;
            rdfs:subClassOf hmas:Organization .

  ?workspace hmas:contains ?slOrgProfile, ?orgProfile .

  filter(?slOrgModel = ?orgModel &&
         ?org != ex:SL_Logistics)
}
order by ?org
