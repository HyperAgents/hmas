@prefix ex: <http://example.org/> .
@prefix hmas: <https://purl.org/hmas/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

#################################################################
#    Core Ontology
#################################################################

## Workspace
ex:LogisticsWS a hmas:Workspace ;
    hmas:contains ex:John, ex:Alice, ex:Bob, ex:Carol, ex:David, ex:Elton, ex:Walter, ex:WarehouseStGallen .

## Agents
ex:John a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:SL_Logistics .
ex:Alice a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:SL_Logistics .
ex:Bob a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:SL_Logistics .
ex:Carol a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:SL_Logistics .
ex:David a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:SL_Logistics .
ex:Elton a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:SL_Logistics .
ex:Walter a hmas:Agent ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMemberOf ex:GL_Logistics .
    
## Artifacts
ex:WarehouseStGallen a hmas:Artifact ;
    hmas:isContainedIn ex:LogisticsWS ;
    hmas:isMaterialOf ex:SL_Logistics .

#################################################################
#    Regulation Ontology
#################################################################

# SPECIFICATION

## NOTE: Regulation entities' specification can be represented
##       using implicit or explicit Target Class SHACL. Here we
##       decided to use implicit Target Class SHACL because we
##       do not expect any extensions for this scenario and we
##       wanted to avoid extra labels.

## Organization Specification
ex:Logistics a sh:NodeShape ;
    rdfs:subClassOf hmas:Organization ;
    sh:property [
        sh:path hmas:hasOrganizationalValue ;
        sh:maxCount 2 ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalValue ;
        sh:hasValue ex:Commitment ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalValue ;
        sh:hasValue ex:IntegrityRespect ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalValue ;
        sh:maxCount 7 ;
        ] ;
    sh:property [
        sh:path hmas:providesRole ;
        sh:hasValue ex:HumanResourcesOfficer ;
        ] ;
    sh:property [
        sh:path hmas:providesRole ;
        sh:hasValue ex:FinanceOfficer ;
        ] ;
    sh:property [
        sh:path hmas:providesRole ;
        sh:hasValue ex:AccountManager ;
        ] ;
    sh:property [
        sh:path hmas:providesRole ;
        sh:hasValue ex:Planner ;
        ] ;
    sh:property [
        sh:path hmas:providesRole ;
        sh:hasValue ex:Carrier ;
        ] ;
    sh:property [
        sh:path hmas:providesRole ;
        sh:hasValue ex:Collector ;
        ] ;
    sh:property [
        sh:path hmas:providesRole ;
        sh:hasValue ex:Deliverer ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalGoal ;
        sh:qualifiedValueShape [ sh:class ex:HumanResourcesManaged ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalGoal ;
        sh:qualifiedValueShape [ sh:class ex:FinancialManaged ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalGoal ;
        sh:qualifiedValueShape [ sh:class ex:CommercialManaged ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalGoal ;
        sh:qualifiedValueShape [ sh:class ex:PlanningManaged ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalGoal ;
        sh:qualifiedValueShape [ sh:class ex:CarriedGoods ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalGoal ;
        sh:qualifiedValueShape [ sh:class ex:CollectedGoods ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:hasOrganizationalGoal ;
        sh:qualifiedValueShape [ sh:class ex:DeliveredGoods ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:proposesFacility ;
        sh:qualifiedValueShape [ sh:class ex:PickingArea ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] ;
    sh:property [
        sh:path hmas:proposesFacility ;
        sh:qualifiedValueShape [ sh:class ex:ReceivingArea ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
        ] .

ex:Manufacturing a sh:NodeShape ;
    rdfs:subClassOf hmas:Organization .

## Organizational Goals
ex:HumanResourcesManaged a sh:NodeShape ;
    rdfs:subClassOf hmas:OrganizationalGoal .
ex:FinancialManaged a sh:NodeShape ;
    rdfs:subClassOf hmas:OrganizationalGoal .
ex:CommercialManaged a sh:NodeShape ;
    rdfs:subClassOf hmas:OrganizationalGoal .
ex:PlanningManaged a sh:NodeShape ;
    rdfs:subClassOf hmas:OrganizationalGoal .
ex:CarriedGoods a sh:NodeShape ;
    rdfs:subClassOf hmas:OrganizationalGoal .
ex:CollectedGoods a sh:NodeShape ;
    rdfs:subClassOf hmas:OrganizationalGoal .
ex:DeliveredGoods a sh:NodeShape ;
    rdfs:subClassOf hmas:OrganizationalGoal .

## Facilities
ex:PickingArea a sh:NodeShape ;
    rdfs:subClassOf hmas:Facility .
ex:ReceivingArea a sh:NodeShape ;
    rdfs:subClassOf hmas:Facility .

# INSTANTIATION

## Organizational Values
ex:Commitment a hmas:OrganizationalValue ;
    rdfs:comment "Do everything you can to keep the organization commitment to the customers."@en ;
    rdfs:label "commitment"@en, "engagement"@fr .

ex:IntegrityRespect a hmas:OrganizationalValue ;
    rdfs:comment "Communicate openly and honestly with the customers and other members of the organization."@en ;
    rdfs:label "integrity and respect"@en, "intégrité et respect"@fr .
    
## Roles
ex:HumanResourcesOfficer a hmas:Role .
ex:FinanceOfficer a hmas:Role .
ex:AccountManager a hmas:Role .
ex:Planner a hmas:Role .
ex:Carrier a hmas:Role .
ex:Collector a hmas:Role .
ex:Deliverer a hmas:Role .

## Organizational Goals
ex:SL_HumanResourcesManaged a ex:HumanResourcesManaged .
ex:SL_FinancialManaged a ex:FinancialManaged .
ex:SL_CommercialManaged a ex:CommercialManaged .
ex:SL_PlanningManaged a ex:PlanningManaged .
ex:SL_CarriedGoods a ex:CarriedGoods .
ex:SL_CollectedGoods a ex:CollectedGoods .
ex:SL_DeliveredGoods a ex:DeliveredGoods .

## Facility
ex:SL_PickingArea a ex:PickingArea .
ex:SL_ReceivingArea a ex:ReceivingArea .

## Organizations
ex:SL_LogisticsProfile a hmas:ResourceProfile .

ex:SL_Logistics a ex:Logistics ;
    hmas:hasProfile ex:SL_LogisticsProfile ;
    hmas:hasOrganizationalValue ex:Commitment, ex:IntegrityRespect ;
    hmas:providesRole ex:HumanResourcesOfficer, ex:FinanceOfficer, ex:AccountManager, ex:Planner, ex:Carrier, ex:Collector, ex:Deliverer ;
    hmas:OrganizationalGoal ex:SL_HumanResourcesManaged, ex:SL_FinancialManaged, ex:SL_CommercialManaged, ex:SL_PlanningManaged, ex:SL_CarriedGoods, ex:SL_CollectedGoods, ex:SL_DeliveredGoods ;
    hmas:proposesFacility ex:SL_PickingArea, ex:SL_ReceivingArea .

ex:GL_LogisticsProfile a hmas:ResourceProfile .

ex:GL_Logistics a ex:Logistics ;
    hmas:hasProfile ex:GL_LogisticsProfile .

ex:HSG_FactoryProfile a hmas:ResourceProfile .

ex:HSG_Factory a ex:Manufacturing ;
    hmas:hasProfile ex:HSG_FactoryProfile .

ex:ITM_FactoryProfile a hmas:ResourceProfile .

ex:ITM_Factory a ex:Manufacturing ;
    hmas:hasProfile ex:ITM_FactoryProfile .
